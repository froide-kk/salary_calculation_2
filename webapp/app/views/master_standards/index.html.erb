<p id="notice"><%= notice %></p>

<h1>Master Standards</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @master_standards.each do |master_standard| %>
      <tr>
        <td><%= master_standard.index %></td>
        <td><%= master_standard.name %></td>
        <td><%= master_standard.value %></td>
        <td><%= link_to 'Show', master_standard %></td>
        <td><%= link_to 'Edit', edit_master_standard_path(master_standard) %></td>
        <td><%= link_to 'Destroy', master_standard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Master Standard', new_master_standard_path %>
