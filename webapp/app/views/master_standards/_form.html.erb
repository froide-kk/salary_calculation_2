<%= form_with(model: master_standard, local: true) do |form| %>
  <% if master_standard.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(master_standard.errors.count, "error") %> prohibited this master_standard from being saved:</h2>

      <ul>
      <% master_standard.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <table class ="seting">
      <tr>
        <td><%= form.label :index %></td>
