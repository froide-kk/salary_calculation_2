<p id="notice"><%= notice %></p>

<h1>Test4s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Test2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test4s.each do |test4| %>
      <tr>
        <td><%= test4.name %></td>
        <td><%= test4.test2.age %></td>
        <td><%= link_to 'Show', test4 %></td>
