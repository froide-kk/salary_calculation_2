<p id="notice"><%= notice %></p>

<h1>User Points</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Master additionalpoint</th>
      <th>Value</th>
      <th>Reason</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_points.each do |user_point| %>
      <tr>
        <td><%= user_point.index %></td>
        <td><%= user_point.master_additionalpoint_id %></td>
        <td><%= user_point.value %></td>
        <td><%= user_point.reason %></td>
        <td><%= link_to 'Show', user_point %></td>
        <td><%= link_to 'Edit', edit_user_point_path(user_point) %></td>
        <td><%= link_to 'Destroy', user_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Point', new_user_point_path %>
