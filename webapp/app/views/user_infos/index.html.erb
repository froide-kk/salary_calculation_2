<p id="notice"><%= notice %></p>

<h1>User Infos</h1>
<h2><%= link_to 'HOME', user_infos_path %></h2>
<%= link_to 'test', user_points_path %>
<table>
  <thead>
    <tr>
      <th>Master position</th>
      <th>Master job</th>
      <th>Index</th>
      <th>Name</th>
      <th>Birth</th>
      <th>Age</th>
      <th>Agep</th>
      <th>Age adjustment</th>
      <th>Insurance</th>
      <th>Address</th>
      <th>Partner num</th>
      <th>Family num</th>
      <th>Shorter working hour ch</th>
      <th>Secondhalf salary</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_infos.each do |user_info| %>
      <tr>
        <td><%= user_info.master_position_id %></td>
        <td><%= user_info.master_job_id %></td>
        <td><%= user_info.index %></td>
        <td><%= user_info.name %></td>
        <td><%= user_info.birth %></td>
        <td><%= user_info.age %></td>
        <td><%= user_info.ageP %></td>
        <td><%= user_info.age_adjustment %></td>
        <td><%= user_info.insurance %></td>
        <td><%= user_info.address %></td>
        <td><%= user_info.partner_num %></td>
        <td><%= user_info.family_num %></td>
        <td><%= user_info.shorter_working_hour_ch %></td>
        <td><%= user_info.Secondhalf_salary %></td>
        <td><%= link_to 'Show', user_info %></td>
        <td><%= link_to 'Edit', edit_user_info_path(user_info) %></td>
        <td><%= link_to 'Destroy', user_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Info', new_user_info_path %>
