<!-- Add your site or app content here -->

  <div class="section-container">
    <div class="container">
      <div class="row">
        <div class="col-xs-12 col-md-8 col-md-offset-2">
          <div class="text-center">
            <h2>All Employee Information</h2>
            <div class="col-xs-12">
                <%= link_to 'Salary',save_infos_path ,style:'font-size:15px;',class:"btn btn-primary"%>
            </div>

            <p id="notice"><%= notice %></p>
          </div>
       </div>
      </div>
  <!--<%= link_to 'New User Info', new_user_info_path %>class="btn btn-default btn-lg" ,-->
    </div>
    <br>
    <table style="margin-left: auto; margin-right: auto;">
    <thead>
      <tr>
        <th>Index</th>
        <th>position</th>
        <th>Name</th>
        <th>Birth</th>
        <th>Age</th>
        <th>Agep</th>
        <th>Age adjustment</th>
        <th>Insurance</th>
        <th>Address</th>
        <th>Partner num</th>
        <th>Family num</th>
        <th>Shorter working hour ch</th>
        <th>Secondhalf salary</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
    <% @position_index = Array.new %>
    <% @position_name = Array.new %>
    <% @master_positions .each do |master_position| %>
      <%= @position_index.push(master_position.id) %>
      <% @position_name.push(master_position.name) %>
    <% end %>
    <% @user_infos.each do |user_info| %>
      <tr>
        <td><%= user_info.index %></td>
        <% @p_index = user_info.master_position_id %>
        <% @p_index.to_i %>
        <td><%= @position_name.at(0) %></td>
        <td><%= user_info.name %></td>
        <td><%= user_info.birth %></td>
        <td><%= user_info.age %></td>
        <td><%= user_info.ageP %></td>
        <td><%= user_info.age_adjustment %></td>
        <td><%= user_info.insurance %></td>
        <td><%= user_info.address %></td>
        <td><%= user_info.partner_num %></td>
        <td><%= user_info.family_num %></td>
        <td><%= user_info.shorter_working_hour_ch %></td>
        <td><%= user_info.Secondhalf_salary %></td>
        <td><%= link_to 'Show', user_info %></td>
        <td><%= link_to 'Edit', edit_user_info_path(user_info) %></td>
        <td><%= link_to 'Destroy', user_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
   <% end %>
   </tbody>
   </table>

    <br>
    <div class="button-css-cy">
      <%= link_to 'New User Info', new_user_info_path ,style:'font-size:15px;',class:"btn btn-primary"%>
    </div>
  </div>



